mixin SSWSlideNavigation(data_name)
    // nav.w-sn 
    nav.w-sn(x-data=data_name, x-cloak)&attributes(attributes)
        .w-sn__wrapper
            block
    // ## nav.w-sn 

mixin SSWSlideNavigationItem(scene_name)
    // .w-sn__item 
    div(x-data="", data-scene=scene_name)
        template(x-if="exists()")
            .w-sn__item(:class="sceneClasses")&attributes(attributes)
                block
    // ## .w-sn__item 

mixin WMultistep(data)
    .w-multistep(
        x-bind="WMultiStep",
        x-data=data
    )
        .w-multistep__wrapper
            .w-multistep__step()

mixin WMultistep__step(step)
    // .w-multistep__step 
    .w-multistep__step(
        x-bind="WMultiStepChild",
        data-multistep=step
    )
        block
    // ## .w-multistep__step 
